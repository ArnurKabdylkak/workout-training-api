extend type Query {
    workouts: [Workout!]!
}

extend type Mutation {
    createWorkout(name: String!): Boolean!
    addExercise(workoutId: ID!, exerciseId: ID!): Boolean!
}

type Workout {
    id: ID!
    name: String!
    exercises: Exercise
}

type Exercise {
    id:  ID!
    name: String!
